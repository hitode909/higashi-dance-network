{
  "name": "higashi-dance-network",
  "version": "1.0.0",
  "description": "http://higashi-dance-network.appspot.com/",
  "main": "index.js",
  "scripts": {
    "build": "browserify public/kasanegi/src/main.js -o public/kasanegi/js/bundle.js",
    "watch:main": "watchify public/kasanegi/src/main.js -o public/kasanegi/js/bundle.js --verbose",
    "watch:sw": "watchify public/kasanegi/src/ServiceWorker.js -o public/kasanegi-service-worker.js --verbose",
    "watch": "npm-run-all -p watch:*",
    "lint": "eslint public/kasanegi/src/* && flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitode909/higashi-dance-network.git"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "flow"
          ]
        }
      ],
      "browserify-global-shim"
    ]
  },
  "browserify-global-shim": {
    "jquery": "$",
    "underscore": "_"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hitode909/higashi-dance-network/issues"
  },
  "homepage": "https://github.com/hitode909/higashi-dance-network#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-global-shim": "^1.0.3",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "flow-bin": "^0.44.2",
    "flow-typed": "^2.1.1",
    "watchify": "^3.9.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {}
}
